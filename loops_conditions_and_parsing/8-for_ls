#!/usr/bin/env bash
# Loop through all non-hidden files in the current directory
for file in *; do
  # Check if there is a dash in the filename
  if [[ "$file" == *-* ]]; then
    # Cut the part of the filename after the first dash and print it
    echo "${file#*-}"
  else
    # If there's no dash, print the full filename
    echo "$file"
  fi
done

